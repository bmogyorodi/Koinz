        app:layout_constraintTop_toBottomOf="@+id/conpeny"
        app:srcCompat="@android:drawable/arrow_down_float" />

    <ImageButton
        android:id="@+id/downshil"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="2dp"
        android:layout_marginEnd="23dp"
        app:layout_constraintBottom_toBottomOf="@+id/downdolr"
        app:layout_constraintEnd_toStartOf="@+id/downdolr"
        app:srcCompat="@android:drawable/arrow_down_float" />

    <ImageButton
        android:id="@+id/downdolr"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="28dp"
        android:layout_marginEnd="89dp"
        android:layout_marginStart="89dp"
        app:layout_constraintBottom_toTopOf="@+id/exchange"
        app:layout_constraintEnd_toEndOf="@+id/goldfromex"
        app:layout_constraintStart_toEndOf="@+id/textView10"
        app:srcCompat="@android:drawable/arrow_down_float" />

    <ImageButton
        android:id="@+id/downquid"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="24dp"
        app:layout_constraintEnd_toStartOf="@+id/downshil"
        app:layout_constraintTop_toTopOf="@+id/downshil"
        app:srcCompat="@android:drawable/arrow_down_float" />

    <TextView
        android:id="@+id/conpeny"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginTop="10dp"
        android:text="0"
        app:layout_constraintStart_toStartOf="@+id/uppeny"
        app:layout_constraintTop_toBottomOf="@+id/uppeny" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="0dp"
        android:layout_height="35dp"
        android:layout_marginBottom="48dp"
        android:layout_marginEnd="10dp"
        android:layout_marginTop="52dp"
