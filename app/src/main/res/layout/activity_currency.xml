        app:layout_constraintBottom_toTopOf="@+id/downshil"
        app:layout_constraintEnd_toEndOf="@+id/upshil" />

    <TextView
        android:id="@+id/condolr"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginBottom="1dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="1dp"
        android:text="0"
        app:layout_constraintBottom_toBottomOf="@+id/conshil"
        app:layout_constraintStart_toStartOf="@+id/updolr"
        app:layout_constraintTop_toTopOf="@+id/conshil" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Peny"
        app:layout_constraintBottom_toTopOf="@+id/uppeny"
        app:layout_constraintEnd_toEndOf="@+id/uppeny" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="39dp"
        android:layout_height="23dp"
        android:layout_marginBottom="105dp"
        android:layout_marginEnd="51dp"
        android:layout_marginStart="51dp"
        android:layout_marginTop="104dp"
