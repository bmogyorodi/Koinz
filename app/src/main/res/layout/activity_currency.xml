        app:layout_constraintBottom_toBottomOf="@+id/downquid"
        app:layout_constraintEnd_toEndOf="@+id/conshil"
        app:layout_constraintStart_toEndOf="@+id/conpeny"
        app:layout_constraintTop_toTopOf="@+id/createcur" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="2dp"
        android:text="Shil"
        app:layout_constraintBottom_toTopOf="@+id/upshil"
        app:layout_constraintEnd_toEndOf="@+id/upshil" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Dolr"
        app:layout_constraintBottom_toTopOf="@+id/updolr"
        app:layout_constraintEnd_toEndOf="@+id/updolr" />

    <android.support.constraint.Group
        android:id="@+id/group"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toTopOf="parent"
        app:layout_constraintEnd_toStartOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <android.support.constraint.Group
        android:id="@+id/group2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toTopOf="parent"
        app:layout_constraintEnd_toStartOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/treasury"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="missingval"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/createcur"
        android:layout_width="101dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="147dp"
        android:layout_marginTop="147dp"
