        app:layout_constraintBottom_toBottomOf="@+id/goldfromex"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/updolr"
        app:layout_constraintTop_toBottomOf="@+id/createcur" />

    <TextView
        android:id="@+id/goldfromex"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="44dp"
        android:layout_marginEnd="37dp"
        android:layout_marginTop="44dp"
        android:text="0000"
        app:layout_constraintBottom_toBottomOf="@+id/downdolr"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView11" />

    <TextView
        android:id="@+id/conquid"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
