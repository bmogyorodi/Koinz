        app:layout_constraintBottom_toTopOf="@+id/downdolr"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/exrate" />

    <TextView
        android:id="@+id/exrate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="1dp"
        android:text="missingex"
        app:layout_constraintEnd_toEndOf="@+id/treasury"
        app:layout_constraintTop_toBottomOf="@+id/treasury" />

</android.support.constraint.ConstraintLayout>
